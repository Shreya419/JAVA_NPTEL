class testarray{
    public static void main (String[] args){
        
        int a[] = {10, 20, 30, 40, 50}; //initialisation
        //displaying Array 
        for(int i = 0; i < a.length; i++){
            System.out.println(a[i]);
        }
        //average
        float sum = 0, avg;
        for(int i = 0; i <a.length; i++){
            sum = sum + a[i];
        }
        avg = sum/a.length;
        System.out.println("average = " + avg);
    }
}
